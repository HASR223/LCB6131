  <script>
  const defaultConfig = {
    dealership_name: "Yuniku Motors",
    hero_headline: "Find The Car Just For You",
    hero_subtext: "Browse our premium selection",
    looking_cta_title: "Are You Looking For a Car?",
    selling_cta_title: "Do You Want to Sell a Car?",
    location_1: "Lot 123, Jalan Gadong, Bandar Seri Begawan",
    location_2: "Lot 456, Jalan Tutong, Tutong District",
    phone_1: "+673 123 4567",
    phone_2: "+673 234 5678",
    phone_3: "+673 345 6789"
  };

  async function onConfigChange(config) {
    const dealershipName = config.dealership_name || defaultConfig.dealership_name;
    const heroHeadline = config.hero_headline || defaultConfig.hero_headline;
    const heroSubtext = config.hero_subtext || defaultConfig.hero_subtext;
    const lookingCtaTitle = config.looking_cta_title || defaultConfig.looking_cta_title;
    const sellingCtaTitle = config.selling_cta_title || defaultConfig.selling_cta_title;
    const location1 = config.location_1 || defaultConfig.location_1;
    const location2 = config.location_2 || defaultConfig.location_2;
    const phone1 = config.phone_1 || defaultConfig.phone_1;
    const phone2 = config.phone_2 || defaultConfig.phone_2;
    const phone3 = config.phone_3 || defaultConfig.phone_3;

    const navDealershipName = document.getElementById('nav-dealership-name');
    if (navDealershipName) navDealershipName.textContent = dealershipName;

    const heroHeadlineEl = document.getElementById('hero-headline');
    if (heroHeadlineEl) heroHeadlineEl.textContent = heroHeadline;

    const heroSubtextEl = document.getElementById('hero-subtext');
    if (heroSubtextEl) heroSubtextEl.textContent = heroSubtext;

    const lookingCtaTitleEl = document.getElementById('looking-cta-title');
    if (lookingCtaTitleEl) lookingCtaTitleEl.textContent = lookingCtaTitle;

    const sellingCtaTitleEl = document.getElementById('selling-cta-title');
    if (sellingCtaTitleEl) sellingCtaTitleEl.textContent = sellingCtaTitle;

    const location1El = document.getElementById('location-1');
    if (location1El) location1El.textContent = location1;

    const location2El = document.getElementById('location-2');
    if (location2El) location2El.textContent = location2;

    const phone1El = document.getElementById('phone-1');
    if (phone1El) phone1El.textContent = phone1;

    const phone2El = document.getElementById('phone-2');
    if (phone2El) phone2El.textContent = phone2;

    const phone3El = document.getElementById('phone-3');
    if (phone3El) phone3El.textContent = phone3;
  }

  if (window.elementSdk) {
    window.elementSdk.init({
      defaultConfig: defaultConfig,
      onConfigChange: onConfigChange,
      mapToCapabilities: (config) => ({
        recolorables: [],
        borderables: [],
        fontEditable: undefined,
        fontSizeable: undefined
      }),
      mapToEditPanelValues: (config) => new Map([
        ["dealership_name", config.dealership_name || defaultConfig.dealership_name],
        ["hero_headline", config.hero_headline || defaultConfig.hero_headline],
        ["hero_subtext", config.hero_subtext || defaultConfig.hero_subtext],
        ["looking_cta_title", config.looking_cta_title || defaultConfig.looking_cta_title],
        ["selling_cta_title", config.selling_cta_title || defaultConfig.selling_cta_title],
        ["location_1", config.location_1 || defaultConfig.location_1],
        ["location_2", config.location_2 || defaultConfig.location_2],
        ["phone_1", config.phone_1 || defaultConfig.phone_1],
        ["phone_2", config.phone_2 || defaultConfig.phone_2],
        ["phone_3", config.phone_3 || defaultConfig.phone_3]
      ])
    });
  }

  function navigateTo(pageId) {
    document.querySelectorAll('.page').forEach(page => {
      page.classList.remove('active');
    });
    
    const targetPage = document.getElementById(pageId);
    if (targetPage) {
      targetPage.classList.add('active');
      window.scrollTo(0, 0);
    }
  }

  function applyFiltersAndNavigate() {
    const year = document.getElementById('filter-year').value;
    const brand = document.getElementById('filter-brand').value;
    const mileage = document.getElementById('filter-mileage').value;
    const price = document.getElementById('filter-price').value;
    
    document.getElementById('inventory-filter-year').value = year;
    document.getElementById('inventory-filter-brand').value = brand;
    document.getElementById('inventory-filter-mileage').value = mileage;
    document.getElementById('inventory-filter-price').value = price;
    
    navigateTo('inventory');
    applyInventoryFilters();
  }

  function applyInventoryFilters() {
    const year = document.getElementById('inventory-filter-year').value;
    const brand = document.getElementById('inventory-filter-brand').value;
    const mileage = document.getElementById('inventory-filter-mileage').value;
    const price = document.getElementById('inventory-filter-price').value;
    
    const cards = document.querySelectorAll('.car-card');
    let visibleCount = 0;
    
    cards.forEach(card => {
      let show = true;
      
      if (year && card.dataset.year !== year) {
        show = false;
      }
      
      if (brand && card.dataset.brand !== brand) {
        show = false;
      }
      
      if (mileage) {
        const carMileage = parseInt(card.dataset.mileage);
        if (mileage === 'Under 30K' && carMileage >= 30000) show = false;
        if (mileage === '30K - 60K' && (carMileage < 30000 || carMileage >= 60000)) show = false;
        if (mileage === '60K - 100K' && (carMileage < 60000 || carMileage >= 100000)) show = false;
        if (mileage === '100K+' && carMileage < 100000) show = false;
      }
      
      if (price) {
        const carPrice = parseInt(card.dataset.price);
        if (price === 'Under $300/month' && carPrice >= 300) show = false;
        if (price === '$300 - $500/month' && (carPrice < 300 || carPrice >= 500)) show = false;
        if (price === '$500 - $700/month' && (carPrice < 500 || carPrice >= 700)) show = false;
        if (price === '$700+/month' && carPrice < 700) show = false;
      }
      
      card.style.display = show ? '' : 'none';
      if (show) visibleCount++;
    });
    
    if (visibleCount === 0) {
      showMessage('No cars match your filters');
    } else {
      showMessage(`Showing ${visibleCount} car${visibleCount !== 1 ? 's' : ''}`);
    }
  }

  function clearInventoryFilters() {
    document.getElementById('inventory-filter-year').value = '';
    document.getElementById('inventory-filter-brand').value = '';
    document.getElementById('inventory-filter-mileage').value = '';
    document.getElementById('inventory-filter-price').value = '';
    
    const cards = document.querySelectorAll('.car-card');
    cards.forEach(card => {
      card.style.display = '';
    });
    
    showMessage('Filters cleared');
  }

  function handleSellSubmit(event) {
    event.preventDefault();
    document.getElementById('sell-form').style.display = 'none';
    document.getElementById('sell-success-message').classList.remove('hidden');
  }

  function handleContactSubmit(event) {
    event.preventDefault();
    document.getElementById('contact-form').style.display = 'none';
    document.getElementById('contact-success-message').classList.remove('hidden');
  }

  function showMessage(message) {
    const messageDiv = document.createElement('div');
    messageDiv.className = 'fixed top-4 right-4 bg-purple-600 text-white px-6 py-3 rounded-lg shadow-lg z-50';
    messageDiv.textContent = message;
    document.body.appendChild(messageDiv);
    
    setTimeout(() => {
      messageDiv.remove();
    }, 3000);
  }

  let darkMode = false;

  function toggleDarkMode() {
    darkMode = !darkMode;
    if (darkMode) {
      document.body.classList.add('dark-mode');
      document.querySelectorAll('.dark-mode-icon').forEach(icon => {
        icon.textContent = '‚òÄÔ∏è';
      });
      const mainIcon = document.getElementById('dark-mode-icon');
      if (mainIcon) mainIcon.textContent = '‚òÄÔ∏è';
    } else {
      document.body.classList.remove('dark-mode');
      document.querySelectorAll('.dark-mode-icon').forEach(icon => {
        icon.textContent = 'üåô';
      });
      const mainIcon = document.getElementById('dark-mode-icon');
      if (mainIcon) mainIcon.textContent = 'üåô';
    }
  }

  const photoGalleries = {
    'recent-1': { current: 0, total: 5 },
    'recent-2': { current: 0, total: 5 },
    'recent-3': { current: 0, total: 5 },
    'popular-1': { current: 0, total: 5 },
    'popular-2': { current: 0, total: 5 },
    'popular-3': { current: 0, total: 5 },
    'inv-1': { current: 0, total: 5 },
    'inv-2': { current: 0, total: 5 },
    'inv-3': { current: 0, total: 5 },
    'inv-4': { current: 0, total: 5 },
    'inv-5': { current: 0, total: 5 },
    'inv-6': { current: 0, total: 5 }
  };

  function selectPhoto(carId, index) {
    photoGalleries[carId].current = index;
    updateGallery(carId);
  }

  function nextPhoto(carId) {
    photoGalleries[carId].current = (photoGalleries[carId].current + 1) % photoGalleries[carId].total;
    updateGallery(carId);
  }

  function previousPhoto(carId) {
    photoGalleries[carId].current = (photoGalleries[carId].current - 1 + photoGalleries[carId].total) % photoGalleries[carId].total;
    updateGallery(carId);
  }

  function updateGallery(carId) {
    const mainPhoto = document.getElementById(`main-photo-${carId}`);
    const thumbs = document.querySelectorAll(`.photo-thumb-${carId}`);
    const currentIndex = photoGalleries[carId].current;
    
    if (mainPhoto && thumbs.length > 0) {
      const selectedThumb = thumbs[currentIndex];
      
      // Copy the background gradient classes from the selected thumbnail
      const gradientClasses = selectedThumb.className.match(/bg-gradient-to-r from-\S+ to-\S+/g);
      if (gradientClasses) {
        // Remove old gradient classes from main photo
        mainPhoto.className = mainPhoto.className.replace(/bg-gradient-to-r from-\S+ to-\S+/g, '');
        // Add new gradient classes
        mainPhoto.className = mainPhoto.className + ' ' + gradientClasses[0];
      }
      
      // Update the emoji/icon in the main photo
      const emojiMatch = selectedThumb.innerHTML.match(/>(.*?)</);
      if (emojiMatch) {
        mainPhoto.innerHTML = `<span class="text-white text-9xl">${emojiMatch[1]}</span>`;
      }
      
      // Update thumbnail borders
      thumbs.forEach((thumb, idx) => {
        if (idx === currentIndex) {
          thumb.classList.remove('border-transparent', 'hover:border-purple-400');
          thumb.classList.add('border-purple-600');
        } else {
          thumb.classList.remove('border-purple-600');
          thumb.classList.add('border-transparent', 'hover:border-purple-400');
        }
      });
    }
  }
</script>
